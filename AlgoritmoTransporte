var resposta: caractere

Escreval ("É terrestre?")
Leia (resposta)

Se (resposta == sim) entao
	resposta <- null
	Escreval ("Cabe apenas uma pessoa?")
	Leia (Resposta)
	
	Se (resposta == sim) entao
		resposta <- null
		Escreval ("É pesado?")
		Leia (resposta)
		
		Se (resposta == sim) entao
			Escreval ("Então, o transporte escolhido foi o TRATOR")
		senao
			Escreval ("Então, o transporte escolhido foi a BICICLETA")
		FimSe
	senao
				
		resposta <- null
		Escreval ("Usa capacete?")
		Leia (resposta)

		Se (resposta == sim) entao
			Escreval ("Então, o transporte escolhido foi a MOTO")
		senao
		
			resposta <- null
			Escreval ("Usa trilho?")
			Leia (resposta)

			Se (resposta == sim) entao
				Escreval ("Então, o transporte escolhido foi o TREM")
			senao
			
				resposta <- null
				Escreval ("É alto?")
				Leia (resposta)
				
				Se (resposta == sim) entao
					resposta <- null
					Escreval ("Usa carroceria?")
					Leia (resposta)
					
					Se (resposta == sim) entao
						Escreval ("Então, o transporte escolhido foi o CAMINHÃO")
					senao
						Escreval ("Então, o transporte escolhidoi foi o ÔNIBUS")
					FimSe
				senao
